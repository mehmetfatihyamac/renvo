generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Listing {
  id          String   @id @default(cuid())
  title       String
  description String
  pricePerDay Int
  delivery    String
  city        String
  createdAt   DateTime @default(now())
}
